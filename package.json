{
    "name":  "flyguardian-backend",
    "version":  "0.1.0",
    "private":  true,
    "type":  "module",
    "scripts":  {
                    "dev":  "set TS_NODE_TRANSPILE_ONLY=1\u0026\u0026 node --watch --loader ts-node/esm src/server.fg.ts",
                    "build":  "tsc -p tsconfig.json",
                    "start":  "node dist/server.fg.js",
                    "typecheck":  "tsc --noEmit",
                    "test":  "jest --config jest.config.fg.cjs",
                    "test:fg":  "jest --config jest.config.fg.cjs --passWithNoTests",
                    "test:auth":  "jest --config jest.config.fg.cjs --testPathPattern=tests/auth.test.fg.ts",
                    "test:ops":  "jest --config jest.config.fg.cjs --testPathPattern=tests/drone.mission.test.fg.ts"
                },
    "dependencies":  {
                         "argon2":  "^0.40.1",
                         "cors":  "^2.8.5",
                         "dotenv":  "^16.4.5",
                         "express":  "^4.19.2",
                         "express-rate-limit":  "^7.4.0",
                         "helmet":  "^7.1.0",
                         "jsonwebtoken":  "^9.0.2",
                         "mongoose":  "^8.6.0",
                         "pino":  "^9.3.2",
                         "pino-pretty":  "^11.2.2",
                         "zod":  "^3.23.8"
                     },
    "devDependencies":  {
                            "@types/cors":  "^2.8.17",
                            "@types/express":  "^4.17.21",
                            "@types/jest":  "^29.5.12",
                            "@types/jsonwebtoken":  "^9.0.6",
                            "@types/node":  "^20.12.12",
                            "@types/supertest":  "^6.0.2",
                            "jest":  "^29.7.0",
                            "mongodb-memory-server":  "^10.1.4",
                            "supertest":  "^6.3.4",
                            "ts-jest":  "^29.2.5",
                            "ts-node":  "^10.9.2",
                            "ts-node-dev":  "^2.0.0",
                            "typescript":  "^5.4.5"
                        }
}